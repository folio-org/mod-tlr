  "description": "Item search result response",
  "type": "object",
  "properties": {
    "id": {
      "type": "string",
      "description": "Item ID"
    },
    "hrid": {
      "type": "string",
      "description": "Item HRID"
    },
    "tenantId": {
      "type": "string",
